Package: SpatialDDLS
Type: Package
Title: Deconvolution of Spatial Transcriptomics Data Based on Neural
        Networks
Version: 1.0.0
Authors@R: c(
  person(given = "Diego", family = "Ma単anes", email = "dmananesc@cnic.es", role = c("aut", "cre"), comment = c(ORCID = "0000-0001-7247-6794")),
  person(given = "Carlos", family = "Torroja", email = "ctorroja@cnic.es", role = "aut", comment = c(ORCID = "0000-0001-8914-3400")),
